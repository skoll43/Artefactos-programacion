<script type="text/javascript">
    
function HeightArticleLight() {
    var ArtLightHeight = $('.page').height();
    $('.cont_articlelight').css({'height': ArtLightHeight});
}

$(document).ready(function () {

    $('.bt_menu_mer_n').click(function() {
        $("nav.vertical").show();
    }); 

    $("nav.vertical").hover(
        function(){
            //$example.stop().animate({opacity: 1}, 400, "linear");
            //$col.stop().animate({ width: "200px" }, 400, "linear");
        },
        function(){
            //$example.stop().animate({ opacity: 0.3 }, 400, "linear");        
            //$col.stop().animate({ width: "0" }, 400, "linear");
            $("nav.vertical").hide();
        }
    ); 

    $('.close_html').click(function() {

      if($(".close_html").hasClass("open")) {
        
        $('.cont_articlelight').addClass('lateralRightBar');
        $('.page').addClass('fullpage');
        $('.close_html').addClass('hide_html');
        $('.close_html').removeClass('open');
        $('.cont_articlelight').hide();
      } else {

        $('.cont_articlelight').removeClass('lateralRightBar');
        $('.page').removeClass('fullpage');
        $('.close_html').removeClass('hide_html');
        $('.close_html').addClass('open');
        $('.cont_articlelight').show();
      }

    });


    $(window).resize(function () {
      HeightArticleLight();
    });
    
    $("#openPram").click(function () {
      $('#modal_limit_articulos').modal('show');
    });

});   

</script>

  <script>
	function canUseWebP() {
            if (CheckPDFActive) return;
    var elem = document.createElement('canvas');

    if (!!(elem.getContext && elem.getContext('2d'))) {
        // was able or not to get WebP representation
        return elem.toDataURL('image/webp').indexOf('data:image/webp') == 0;
    }
    else {
        // very old browser like IE 8, canvas not supported
        return false;
    }
}
var hasWebP = false;
    hasWebP = true;
if (canUseWebP() && hasWebP && !CheckPDFActive) {

	$('.fullImg').each(function(index) { 
		$(this).attr('src',$(this).attr('src-img').replace('.jpg','.webp'))
	});
} else {
        if (!CheckPDFActive)
            $('.fullImg').each(function(index) { 
		$(this).attr('src',$(this).attr('src-img'))
            });
}
      $(document).ready(function() {
          //$('nav#menu').mmenu();
          document.addEventListener('contextmenu', function(event) { return event.preventDefault();});
      });
      
      
      $(document).ready(function () {

          var CheckPDFActive = Cookies.get("ActivePDF");

          if (CheckPDFActive != undefined) {

              if (CheckPDFActive == 'active') {

                  // BOTON DESACTIVAR 
                  $('#active_pdf').hide();
                  $('#inactive_pdf').show();
                 
                  $('#viewer_webp').remove();
                  $('#viewer').show();
                  
                  $('.splitToolbarButton').show();
                  setTimeout(function(){ 
                    $('#scaleSelectContainer').show(); 
                  }, 1000);
                  

              } 

          } else {
            $('#viewer').remove();
            $('#active_pdf').show();

            $('.splitToolbarButton').hide();
            $('#scaleSelectContainer').hide();
          }

          $('#active_pdf').click(function() {
              Cookies.set('ActivePDF', 'active' , { path: '/' , expires: 1 });
              location.reload();
              //alert();
          }); 

          $('#inactive_pdf').click(function() {
              Cookies.set('ActivePDF', 'desactive' , { path: '/' , expires: 0 });
              location.reload();
              //alert();
          }); 

          // REMOVER COPIAR | PEGAR
          $('#viewer').bind('copy paste',function(e) {
              e.preventDefault(); return false; 
          });
          
          // NAVEGAR CON TECLADO
          $(document).keydown(function(e) {
              if(e.keyCode == 37) {
                  window.location = 'S43VAU9E#zoom=page-width';
              }
              if(e.keyCode == 39) {
                  window.location = 'I43VASPB#zoom=page-width';
              }
          });

      });

      

